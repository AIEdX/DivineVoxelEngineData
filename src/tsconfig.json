{
  "compilerOptions": {
    "target": "esNext",
    "module": "esNext",
    "rootDir": "./",
    "baseUrl": "./",
    "declaration": true,
    "outDir": "../dist",
    "types": ["node", "divine-binary-tags", "voxelspaces", "zeneithdb"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "moduleResolution": "node"
  }
}
